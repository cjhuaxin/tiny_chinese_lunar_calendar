  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch development (macOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "deviceId": "macos"
    },
    {
      "name": "Launch staging (macOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "deviceId": "macos"
    },
    {
      "name": "Launch production (macOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "deviceId": "macos"
    },
    {
      "name": "Launch development (iOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_development.dart",
      "args": ["--flavor", "development"]
    },
    {
      "name": "Launch staging (iOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_staging.dart",
      "args": ["--flavor", "staging"]
    },
    {
      "name": "Launch production (iOS)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_production.dart",
      "args": ["--flavor", "production"]
    }
  ]
}
